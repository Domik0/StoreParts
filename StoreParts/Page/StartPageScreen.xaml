<Page x:Class="StoreParts.Page.StartPageScreen"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:StoreParts.Page"
      mc:Ignorable="d" 
      d:DesignHeight="680" d:DesignWidth="940"
      Height="680" Width="940"
      Title="StartPageScreen">

    <Grid Background="#FFFFFF">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <TextBlock Grid.Row="0" Text="Новые товары" Foreground="#ff850a" FontWeight="Bold" FontSize="30"
                   VerticalAlignment="Top" HorizontalAlignment="Left" Margin="0,0,0,20"/>
        <TextBlock Grid.Row="1" Text="Товары в корзине" Foreground="#ff850a" FontWeight="Bold" FontSize="30"
                   VerticalAlignment="Top" HorizontalAlignment="Left" Margin="0,0,0,20"/>

    </Grid>
</Page>
